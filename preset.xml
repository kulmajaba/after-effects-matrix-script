<Effect matchname="Pseudo/Matrix Letters" name="$$$/AE/Preset/MatrixLetters=Matrix Letters">
	<Layer name="$$$/AE/Preset/MatrixLetters/TargetLayer=Target Title Layer" default_self="false" />
	<Slider name="$$$/AE/Preset/MatrixLetters/TargetIndex=Target Title Character Index" default="0" valid_min="0" valid_max="1000" slider_min="0" slider_max="50" precision="0" />
	<Slider name="$$$/AE/Preset/MatrixLetters/Length=Length" default="20" valid_min="1" valid_max="1000" slider_min="1" slider_max="50" precision="0" />
	<Slider name="$$$/AE/Preset/MatrixLetters/Lifespan=Character Lifespan" default="10" valid_min="1" valid_max="500" slider_min="1" slider_max="100" precision="0" />
	<Slider name="$$$/AE/Preset/MatrixLetters/MinInterval=Minimum Update Interval" default="2" valid_min="1" valid_max="300" slider_min="1" slider_max="30" precision="0" />
	<Checkbox name="$$$/AE/Preset/MatrixLetters/AllCharactersStatic=All Characters Static" default="false" />
	<Slider name="$$$/AE/Preset/MatrixLetters/ProbOfChanging=Probability of Changing" default="10" valid_min="0" valid_max="100" slider_min="0" slider_max="100" precision="1" />
	<Slider name="$$$/AE/Preset/MatrixLetters/SpaceFrequency=Frequency of Whitespace" default="20" valid_min="0" valid_max="200" slider_min="0" slider_max="100" precision="0" />
	<Color name="$$$/AE/Preset/MatrixLetters/MainColor=Main Color" default_red="67" default_green="237" default_blue="0" />
	<Color name="$$$/AE/Preset/MatrixLetters/HeadColor=Leading Character Color" default_red="255" default_green="255" default_blue="255" />
	<Slider name="$$$/AE/Preset/MatrixLetters/Seed=Random Seed" default="0" valid_min="0" valid_max="100000" slider_min="0" slider_max="1000" precision="0" />
	<Group name="$$$/AE/Preset/MatrixLetters/Font=Font options">
		<Slider name="$$$/AE/Preset/MatrixLetters/FontSize=Font Size" default="90" valid_min="1" valid_max="1000" slider_min="1" slider_max="200" precision="1" />
		<Slider name="$$$/AE/Preset/MatrixLetters/LineHeightMultiplier=Line Height Multiplier" default="0.8125" valid_min="0" valid_max="10" slider_min="0" slider_max="1" precision="4" />
	</Group>
	<Group name="$$$/AE/Preset/MatrixLetters/Head=Head and Tail Options">
		<Slider name="$$$/AE/Preset/MatrixLetters/HeadCharacters=Accented Head Characters" default="1" valid_min="0" valid_max="100" slider_min="1" slider_max="5" precision="0" />
		<Slider name="$$$/AE/Preset/MatrixLetters/HeadFade=Head Accent Fade" default="0" valid_min="0" valid_max="100" slider_min="0" slider_max="100" precision="1" DISPLAY_PERCENT="true" />
		<Slider name="$$$/AE/Preset/MatrixLetters/TailFadeStart=Tail Fade Start Position" default="30" valid_min="0" valid_max="50" slider_min="0" slider_max="50" precision="1" DISPLAY_PERCENT="true"/>
		<Slider name="$$$/AE/Preset/MatrixLetters/TailFadeOpacity=Tail Fade End Opacity" default="10" valid_min="0" valid_max="100" slider_min="0" slider_max="100" precision="1" DISPLAY_PERCENT="true"/>
	</Group>
</Effect>